keycloak:
  auth-server-url: http://localhost:9090
  realm: user-account-service
  client-id: dyc
  client-secret: y1GgLweQKipd0Ro1GuTUlEtUVoHomJ4G
  admin:
    username: admin
    password: password
    client-id: dyc

server:
  host: localhost
  port: 168

logging:
  level:
    org:
      springframework:
        security: INFO

jwt:
  auth:
    converter:
      resource-id: dyc
      principle-attribute: preferred_username
springdoc:
  swagger-ui:
    path: /swagger-ui.html
    enabled: true
  api-docs:
    path: /v3/api-docs
    enabled: true
  show-actuator: false