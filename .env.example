spring:
  application:
    name: user-account-service
    admin-generate:
      email: ${ADMIN_EMAIL}
      password: ${ADMIN_PASSWORD}
      first-name: ${ADMIN_FIRST_NAME}
      last-name: ${ADMIN_LAST_NAME}

  datasource:
    url: jdbc:postgresql://localhost:5432/database
    username: admin
    password: admin
    driver-class-name: org.postgresql.Driver
  r2dbc:
    username: admin
    password: admin
    url: r2dbc:postgresql://localhost:5432/database
